<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dingtao.rrmmp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application

        android:allowBackup="true"
        android:icon="@mipmap/weiduhealth"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/weiduhealth"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.bw.health_setup.activity.Set_Up_Activity"></activity>
        <activity android:name="com.bw.health_homepage.view.activity.LoginActivity"></activity>
        <activity android:name="com.bw.health_homepage.view.activity.BannerWebViewMainActivity"></activity>
        <activity android:name="com.bw.health_homepage.view.activity.MineActivity"></activity>
        <activity android:name="com.bw.health_homepage.view.activity.HomePage_SearchActivity"></activity>
        <activity android:name="com.bw.health_knowledge.activity.Drug_Activity"></activity>
        <activity android:name="com.bw.health_knowledge.activity.Disease_Activity"></activity>
        <activity android:name="com.bw.health_knowledge.activity.Knowledge_Activity"></activity>
        <activity android:name="com.bw.health_bootpage.activity.BootPageActivity"></activity>
        <activity android:name="com.bw.health_homepage.view.activity.MyActivity"></activity>
        <activity android:name="com.bw.health_bootpage.activity.StartUpPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>